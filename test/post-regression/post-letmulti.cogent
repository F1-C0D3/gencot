-- Single binding with simple body

f1 : U32 -> U32
f1 i = let (v1,v2) = (5,i) in v1 + v2

f2 : U32 -> U32
f2 i = let (v1,v2,v3) = (5,i,7) in v1 + v2 * v3 + v1

f3 : U32 -> U32
f3 i = let (v1,_,v2) = (5,i,7) in v1 + v2 + v1

f4 : U32 -> U32
f4 i = let (v1,(),v2) = (5,(),i) in v1 + v2 + v1

f5 : U32 -> U32
f5 i = let (v1,v2) = (5,i) in v1 + i

-- Binding Sequence with simple body

f10 : U32 -> U32
f10 i = let (v1,v2) = (5,i+1) 
        and (v3,v4) = (i-1,v2)
        in v1 + v4

f11 : U32 -> U32
f11 i = let (v1,v2) = (5,i+1) 
        and (v3,i) = (v2 + i, v1) 
        in i + v1 + v2 + v3

f12 : U32 -> U32
f12 i = let (v1,v2,v3) = (i+1,i,i-1) 
        and (v2,v3,v4) = (5,i,v1+i)
        in v1+v3+v4

f13 : U32 -> (U32,U32,U32)
f13 i = let (v1,v2) = (i+1,i-1) 
        and v' = 2*i 
        in (v1,v',v2)

-- Bindings with body with non-let bindings

f20 : U32 -> U32
f20 i = let (v1,v2) = (i+1,i-1) 
        in i | w -> w + v1+v2

f21 : U32 -> U32
f21 i = let (v1,v2) = (i+1,i-1) 
        in i | v1 -> v1 + v2

f22 : U32 -> U32
f22 i = let (v1,v2) = (i,5)
        in (5 | i -> v1 + v2)

-- Binding with complex expression

f30 : U32 -> U32
f30 i = let (v1,v2) = 
          let (v1,v2,v3) = (i,1,2)
          in (v1+v2,v1*v3)
        in v1 + v2

f31 : U32 -> U32
f31 i = let (v1,v2) = 
          if i == 0 then (5,i) else (2*i,10)
        in v1 + v2

f31a : U32 -> U32
f31a i = let (v1,v2) = 
          if i == 0 then (5,i) else (2*i,10)
        in (v1,v2)

f31b : U32 -> U32
f31b i = let (v1,v2) = 
          if i == 0 then (5,i) else (2*i,10)
        in (v2,v1)

f32 : U32 -> U32
f32 i = let (v1,v2) = (7 | i -> (i+1,5))
        in v1+v2

-- Bindings with retainments

g2 : U32 -> (U32,U32)
g3 : U32 -> (U32,U32,U32)

f40 : U32 -> U32
f40 i = let (v1,v2) = g2 i
        in v1 + v2

f40a : U32 -> U32
f40a i = let (v1,v2) = g2 i
        in (v1,v2)

f41 : U32 -> U32
f41 i = let (v1,v2) = (i,5)
        and (i,v) = g2 i
        in v1 + v2 + i

f41a : U32 -> U32
f41a i = let (v1,v2) = (i,5)
        and (i,v) = g2 v1
        in v1 + v2 + i

f41b : U32 -> U32
f41b i = let (v1,v2) = (i,5)
        and (i,v) = g2 v2
        in v1 + v2 + i

f42 : U32 -> U32
f42 i = let (v1,_,v2) = g3 i
        in v1 + v2

-- Nested tuple patterns

f50 : U32 -> U32
f50 i = let (v1,(v2,v3)) = g2 i
        in v1 + v2 + v3

f50a : U32 -> U32
f50a i = let (v1,(v2,v3)) = (i,g2 i)
        in v1 + v2

f50b : U32 -> U32
f50b i = let (v1,(v2,v3)) = (i,g2 i)
        in v1

