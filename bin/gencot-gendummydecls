#! /bin/csh -f

# Generate dummy declarations for preprocessor constants.
# This is a filter from stdin to stdout.

awk \
'{ if ($0 ~ /#define/) { \
     print "int " $2 ";" \
   } \
 }'

# documentation:
#
# Every constant definition of the form
#   #define NNN XXX
# is translated to the C declaration
#   int NNN;
